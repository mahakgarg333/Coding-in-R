> mixbag1 <- c(1,T,2,F)
> mixbag1
[1] 1 1 2 0
> class(mixbag1)
[1] "numeric"
> mixbag2 <- c(1,"a",2,"b")
> mixbag2
[1] "1" "a" "2" "b"
> class(mixbag2)
[1] "character"
> mixbag3 <- c(1,"a",T)
> mixbag3
[1] "1"    "a"    "TRUE"
> #Extracting elements from vector 
> mixbag2
[1] "1" "a" "2" "b"
> mixbag2[1]
[1] "1"
> mixbag2[1:3]
[1] "1" "a" "2"
